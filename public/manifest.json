{
  "manifest_version": 2,
  "name": "Productivity Dashboard",
  "author": "Pratik Tiwari",
  "version": "1.0.0",
  "description": "Productivity Dashboard",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["tabs", "http://localhost/", "http://*/*", "http://*/*"],
  "content_security_policy": "script-src 'self'; 'https://google.com';unsafe-inline; object-src 'self'"
}
